{
  "name": "server",
  "version": "1.0.0",
  "description": "My Graphcool Service",
  "scripts": {
    "deploy": "node -r dotenv/config $(which graphcool) deploy dotenv_config_path=./.env.development",
    "deploy:prod": "node -r dotenv/config $(which graphcool) deploy dotenv_config_path=./.env.production --target prod"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphcool-lib": "^0.1.4",
    "mailgun-js": "^0.22.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "dotenv": "^6.2.0"
  }
}
